{
  "name": "schoolverse_1",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "next dev -p 3000",
    "build": "prisma generate && next build",
    "start": "next start -p 3000",
    "clean:next": "rimraf .next",
    "clean:modules": "rimraf node_modules",
    "clean:all": "rimraf .next node_modules",
    "clean:dev": "npm run clean:next && npm run dev",
    "clean:install": "npm run clean:all && npm install && npm run dev",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "docker:dev": "docker compose -f docker-compose.dev.yml up --build",
    "docker:dev:down": "docker compose -f docker-compose.dev.yml down",
    "docker:prod": "docker compose -f docker-compose.prod.yml up --build -d",
    "docker:prod:down": "docker compose -f docker-compose.prod.yml down",
    "rt:dev": "tsx server/index.ts",
    "rt:start": "tsx server/index.ts",
    "rt:mediasoup": "tsx server/rtc/mediasoupServer.ts",
    "pretest": "npm run prisma:generate",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "pretest:coverage": "npm run prisma:generate",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.4.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^2.4.3",
    "fabric": "^6.7.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.547.0",
    "mediasoup": "^3.19.3",
    "mediasoup-client": "^3.17.1",
    "next": "15.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "three": "^0.168.0",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@next/eslint-plugin-next": "15.0.0",
    "@playwright/test": "^1.49.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.38.0",
    "eslint-config-next": "15.0.0",
    "msw": "^2.4.9",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prisma": "^6.18.0",
    "tailwindcss": "^3.4.12",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^3.2.4"
  }
}
